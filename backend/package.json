{
  "name": "agr-pipeline-backend",
  "version": "1.0.0",
  "description": "Agr Pipeline Backend API Server",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "legacy": "node server.js",
    "generate-sample": "node scripts/generateSampleData.js",
    "test-api": "node scripts/testAPI.js",
    "test": "node tests/runner/testRunner.js",
    "test:all": "node tests/runner/testRunner.js",
    "test:unit": "jest tests/unit --verbose --testTimeout=5000",
    "test:integration": "jest tests/integration --verbose --testTimeout=15000",
    "test:e2e": "jest tests/e2e --verbose --testTimeout=60000",
    "test:coverage": "jest --coverage --collectCoverageFrom='src/**/*.js' --coverageReporters=text,lcov,html,json",
    "test:watch": "jest --watch --testTimeout=10000",
    "test:watch:unit": "jest tests/unit --watch --verbose",
    "test:watch:integration": "jest tests/integration --watch --verbose",
    "test:ci": "jest --coverage --ci --watchAll=false --testTimeout=30000 --maxWorkers=2",
    "test:debug": "node --inspect-brk=9229 node_modules/.bin/jest --runInBand --no-cache",
    "test:quick": "jest tests/unit --verbose --silent",
    "test:specific": "jest --testNamePattern",
    "test:file": "jest --testPathPattern",
    "test:performance": "node tests/runner/testRunner.js performance",
    "test:smoke": "jest tests/smoke --verbose --testTimeout=10000",
    "setup:test": "npm run clean:test && npm run setup:test-dirs && npm run setup:test-db",
    "setup:test-dirs": "node -e \"const fs=require('fs-extra'); ['tests/temp','tests/temp/data','tests/temp/uploads','tests/temp/processed','tests/temp/logs','test-results','coverage'].forEach(dir=>fs.ensureDirSync(dir))\"",
    "setup:test-db": "node -e \"require('fs-extra').ensureDirSync('tests/temp/db')\"",
    "clean:test": "node -e \"const fs=require('fs-extra'); ['tests/temp','test-results','coverage','junit.xml'].forEach(path=>fs.removeSync(path))\"",
    "clean:coverage": "node -e \"require('fs-extra').removeSync('coverage')\"",
    "start:test": "NODE_ENV=test node src/server.js",
    "pretest": "npm run setup:test",
    "posttest": "npm run generate:test-report",
    "pretest:ci": "npm run clean:test && npm run setup:test",
    "posttest:ci": "npm run generate:test-report",
    "generate:test-report": "node -e \"console.log('ðŸ“Š Test execution completed. Check test-results/ for detailed reports.')\"",
    "report:coverage": "npm run test:coverage && npx http-server coverage/lcov-report -p 8080 -o",
    "report:test": "npx http-server test-results -p 8081 -o",
    "validate:tests": "node -e \"const glob=require('glob'); const files=glob.sync('tests/**/*.test.js'); console.log(`Found ${files.length} test files:`, files)\"",
    "lint:tests": "eslint tests/**/*.js --fix",
    "audit:tests": "npm audit --audit-level moderate --include=dev"
  },
  "keywords": [
    "agr",
    "pipeline",
    "api",
    "backend"
  ],
  "author": "Agr Pipeline Team",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "duckdb": "^1.3.2",
    "express": "^4.21.2",
    "form-data": "^4.0.4",
    "fs-extra": "^11.3.0",
    "helmet": "^8.0.0",
    "http-proxy-middleware": "^3.0.5",
    "moment-timezone": "^0.6.0",
    "multer": "^2.0.0",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.10",
    "parquetjs-lite": "^0.8.7",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@jest/test-result": "^29.7.0",
    "@types/jest": "^29.5.12",
    "chalk": "^4.1.2",
    "eslint": "^8.57.1",
    "glob": "^10.4.5",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-html-reporter": "^3.10.2",
    "jest-junit": "^16.0.0",
    "mock-fs": "^5.2.0",
    "nock": "^13.5.1",
    "sinon": "^17.0.1",
    "supertest": "^6.3.4"
  }
}